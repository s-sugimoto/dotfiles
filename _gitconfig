[alias]
  l = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
  lp = log -p
  b = branch
  st = status
  cm = commit -m
  ca = commit -a -m
  co = checkout
  com = checkout master
  cob = checkout -b
  plo = pull origin
  plom = pull origin master
  pso = push origin
  difff = diff --word-diff
  un = reset HEAD
